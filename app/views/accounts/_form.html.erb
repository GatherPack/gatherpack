<%= f.error_notification %>
<%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

<div class="form-inputs">
  <%= f.input :name %>
  <%= f.association :team, collection: policy_scope(Team) %>

  <%= f.label :holder_gids, "Account Holders" %>
  <%= f.select :holder_gids, holder_collection(:account), { include_blank: false }, { multiple: true, data: { controller: "slim" } } %>
</div>
