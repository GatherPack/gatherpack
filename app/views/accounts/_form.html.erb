<%= f.error_notification %>
<%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

<div class="form-inputs">
  <%= f.input :name %>
  <%= f.association :team, collection: policy_scope(Team) %>

  <div class="select optional account_holders">
    <%= f.label :holder_gids, "Account Holders", class: "form-label select optional" %><br>
    <%= f.select :holder_gids, holder_collection(:account), {}, multiple: true %>
  </div>
</div>
