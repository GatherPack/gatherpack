<% breadcrumb :new, "ledger_tag" %>

<%= simple_form_for(@ledger_tag) do |f| %>
<div class="card">
  <div class="card-header">
    <h1>New ledger tag</h1>
  </div>
  <div class="card-body">
    <%= render "form", f: f %>
  </div>
  <div class="card-footer">
    <%= link_to "Cancel", ledger_tags_path, class: 'btn btn-warning' %>
    <%= f.button :submit, class: 'btn btn-primary' %>
  </div>
</div>
<% end %>
