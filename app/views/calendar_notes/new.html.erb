<% breadcrumb :new, "Calendar Note" %>

<%= simple_form_for(@calendar_note) do |f| %>
  <div class="card">
    <div class="card-header">
      <h1>New calendar note</h1>
    </div>
    <div class="card-body">
      <%= render "form", f: f %>
    </div>
    <div class="card-footer">
      <%= link_to "Cancel", calendar_index_path, class: 'btn btn-warning' %>
      <%= f.button :submit, class: 'btn btn-primary' %>
    </div>
  </div>
<% end %>
